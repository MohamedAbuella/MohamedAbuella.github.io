@techreport{devogelaer_costbenefit_2017,
  address = {{Brussels}},
  type = {Report},
  title = {Cost-Benefit Analysis of a Selection of Policy Scenarios on an Adequate Future {{Belgian}} Power System: {{Economic}} Insights on Different Capacity Portfolio and Import Scenarios},
